@model IEnumerable<NetCoreStatik.Models.Events>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.userIp)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.pageName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.enterDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.exitDate)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.userIp)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.pageName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.enterDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.exitDate)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.eventId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.eventId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.eventId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
